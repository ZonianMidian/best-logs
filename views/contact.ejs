<%- include('navbar') -%>
<head>
    <title>Logs | Contact</title>
    <meta name="description" content="Contact with @ZonianMidian" />
    <meta property="og:title" content="Logs | Contact" />
    <meta property="og:image" content="<%= avatar %>" />
    <meta property="og:description" content="Contact with @ZonianMidian" />
</head>
<body>
    <div class="text-center margin">
        <h1>Contact</h1>
        <blockquote class="blockquote margin">
            <img
                src="<%= avatar %>"
                draggable="false"
                title="<%= name %>"
                alt="<%= name %>"
                style="height: 10em"
            />
            <h1 class="title hoster"><%= name %></h1>
            <div class="socials">
                <a
                    href="https://x.com/ZonianMidian"
                    class="fa-brands fa-x-twitter"
                    data-umami-event="twitter"
                ></a>
                <a
                    href="https://www.twitch.tv/ZonianMidian"
                    class="fa-brands fa-twitch"
                    data-umami-event="twitch"
                ></a>
                <a
                    href="https://www.github.com/ZonianMidian"
                    class="fa-brands fa-github"
                    data-umami-event="github"
                ></a>
                <a
                    href="https://discord.gg/rMjftZx8Ex"
                    class="fa-brands fa-discord"
                    data-umami-event="discord"
                ></a>
            </div>
            <p>Contact me for questions or suggestions :P</p>
        </blockquote>
    </div>
</body>
